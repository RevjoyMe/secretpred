# Wallet Connect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Zama FHEVM RPC URL
NEXT_PUBLIC_FHEVM_RPC_URL=https://devnet.zama.ai

# Contract Addresses (update after deployment)
NEXT_PUBLIC_PREDICTION_MARKET_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_ENCRYPTED_BETTING_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_ORACLE_INTEGRATION_ADDRESS=0x0000000000000000000000000000000000000000

# API Configuration
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000/api

# Analytics (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
NEXT_PUBLIC_ENABLE_TESTNETS=true
NEXT_PUBLIC_ENABLE_FAUCET=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Development
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

